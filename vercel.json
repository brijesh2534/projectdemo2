{
  "cleanUrls": true,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/fetchNiftyData",
      "destination": "/api/fetchNiftyData.js"
    }
  ],
  "crons": [
    {
      "path": "/api/fetchNiftyData",
      "schedule": "*/5 * * * *"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
